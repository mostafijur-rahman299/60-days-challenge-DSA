   "source": [
    "nums = [-1,0,3,5,9,12]\n",
    "target = 9\n",
    "\n",
    "l, r = 0, len(nums) - 1\n",
    "\n",
    "while l <= r:\n",
    "    m = (l + r) // 2\n",
    "    if nums[m] > target:\n",
    "        r = m - 1\n",
    "    elif nums[m] < target:\n",
    "        l = m + 1\n",
    "    else:\n",
    "        print(m)\n",
    "        break"
   ]
