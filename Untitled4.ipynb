   "source": [
    "s = [\"h\",\"e\",\"l\",\"l\",\"o\"]\n",
    "\n",
    "left = 0\n",
    "right = len(s) - 1\n",
    "\n",
    "while left < right:\n",
    "    temp = s[right]\n",
    "    s[right] = s[left]\n",
    "    s[left] = temp\n",
    "    \n",
    "    left += 1\n",
    "    right -= 1\n",
    "\n",
    "print(s)"
   ]
